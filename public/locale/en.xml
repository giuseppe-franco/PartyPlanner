				</options>
			</category>
			<notes>
				<label>
					Notes
				</label>
				<placeholder>
					Allergies, preferences, etc.
				</placeholder>
			</notes>
		</fields>
		<buttons>
			<add>
				Add Item
			</add>
			<adding>
				Adding...
			</adding>
		</buttons>
